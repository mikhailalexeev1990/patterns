<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="icon" type="image/png" sizes="16x16"
          href="data:image/png;base64,"/>
    <title>Patterns API</title>
</head>
<body>
<div id="swagger-ui"></div>
</body>

{% include 'swagger/assets/swagger-ui.twig' %}
{% include 'swagger/assets/swagger-ui-standalone-preset.twig' %}
{% include 'swagger/assets/swagger-ui-bundle.twig' %}

<script>
    const CaseInsensitiveFilterPlugin = function (system) {
        return {
            fn: {
                opsFilter: (taggedOps, phrase) => {
                    return taggedOps.filter((tagObj, tag) => tag.toLowerCase().indexOf(phrase.toLowerCase()) !== -1);
                }
            }
        }
    };

    window.onload = function () {
        SwaggerUIBundle({
            url: "{{ url }}",
            dom_id: '#swagger-ui',
            deepLinking: true,
            docExpansion: 'none',
            presets: [
                SwaggerUIBundle.presets.apis,
                SwaggerUIStandalonePreset,
            ],
            plugins: [
                SwaggerUIBundle.plugins.DownloadUrl,
                CaseInsensitiveFilterPlugin,
            ],
            layout: "StandaloneLayout",
            onComplete() {
                addCollapseBtns();
            },
            filter: true,
            tagsSorter: (a, b) => {
                if (/^[а-я]/i.test(a) && !/^[а-я]/i.test(b)) {
                    return -1; // cyrillic first
                }
                if (!/^[а-я]/i.test(a) && /^[а-я]/i.test(b)) {
                    return 1; // latin second
                }
                return a.localeCompare(b, undefined, { sensitivity: 'base' });
            },
        });

        const addCollapseBtns = () => {
            const btn = `
                    <section class="schemes wrapper block col-12" style="padding-top: 35px;">
                        <div class="auth-wrapper">
                            <button class="btn authorize unlocked"
                                    id="collapse-group"
                                    onclick="[...document.querySelectorAll('.opblock-tag.no-desc')].forEach(el => el.dataset.isOpen === 'true' ? el.click() : null);"
                                    style="border-color: #ff6060; color: #ff6060;">
                                <span>Collapse</span>
                                <svg version="1.1" id="Capa_1" x="0px" y="0px" viewBox="0 0 488.4 488.4" style="fill: #ff6060; width: 20px;" xmlns="http://www.w3.org/2000/svg"> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"> <g> <g> <polygon points="393.8,260.9 488.4,260.9 488.4,227.5 393.8,227.5 433.8,188 410.1,164.3 329.9,244.2 410.1,324.1 433.8,300.4 &#10;&#9;&#9;&#9;&#9;&#9;&#9;&#9;"/> <polygon points="94.6,227.5 0,227.5 0,260.9 94.6,260.9 54.6,300.4 78.3,324.1 158.5,244.2 78.3,164.3 54.6,188 &#9;&#9;&#9;"/> <rect x="185.7" y="0" width="42.3" height="488.4"/> <rect x="268.6" y="0" width="42.3" height="488.4"/> </g> </g> </g> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> <g transform="matrix(0, -1, 1, 0, 0, 488.399994)"/> </svg>
                            </button>
                            <button class="btn authorize unlocked"
                                    id="collapse-group"
                                    onclick="[...document.querySelectorAll('.opblock-tag.no-desc')].forEach(el => el.dataset.isOpen === 'false' ? el.click() : null);"
                                    style="border-color: #4990e2; color: #4990e2;">
                                <span>Expand</span>
                                <svg version="1.1" id="Capa_1" x="0px" y="0px" viewBox="0 0 490 490" style="fill: #4990e2; width: 20px;" xmlns="http://www.w3.org/2000/svg"> <g transform="matrix(0, -1, 1, 0, 0, 490)"> <g> <g> <polygon points="425.8,228.3 354.7,228.3 354.7,261.7 425.8,261.7 386.2,301.4 409.9,325.1 490,245 409.9,164.9 386.2,188.6 &#9;&#9;&#9;&#10;&#9;&#9;&#9;&#9;"/> <polygon points="64.2,261.7 135.3,261.7 135.3,228.3 64.2,228.3 103.8,188.6 80.1,164.9 0,245 80.1,325.1 103.8,301.4 &#9;&#9;&#9;"/> <rect x="178.5" width="50.2" height="490"/> <rect x="269.5" width="50.2" height="490"/> </g> </g> </g> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> <g transform="matrix(0, -1, 1, 0, 0, 490)"/> </svg>
                            </button>
                        </div>
                    </section>
                `;
            const div = document.createElement('div');
            div.innerHTML = btn.trim();
            document.querySelector('#swagger-ui .scheme-container').append(div.firstChild);
        }
    }
</script>
</html>
